{
  "name": "eevee",
  "version": "0.1.0",
  "description": "Tools for various games, name inspired by Eve Online",
  "type": "module",
  "engines": {
    "node": "22.x"
  },
  "main": "main.js",
  "scripts": {
    "local": "npm run build && heroku local web --port 5001",
    "start": "node ./build/main.js",
    "refresh-discord": "node ./discord/scripts/refresh-discord-commands.js",
    "logs": "heroku logs --tail",
    "lint": "eslint .",
	"build": "npm run lint && npx tsc"
  },
  "dependencies": {
    "cool-ascii-faces": "^1.3.4",
    "discord.js": "^14.15.3",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.9",
    "eslint": "^9.8.0",
    "express": "^4.15.2",
    "pg": "^8.12.0"
  },
  "devDependencies": {
    "tape": "^5.8.1",
    "typescript": "^5.5.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Ilandria/Eevee"
  },
  "keywords": [
    "node",
    "heroku",
    "express"
  ],
  "license": ""
